{
  "compilerOptions": {
    "target": "es2021",                                 
    "module": "commonjs",                                
    "moduleResolution": "node",                       
    "resolveJsonModule": true,                        
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "noImplicitAny": true,                           
    "skipLibCheck": true                                
  }
}
